{% extends '@App/Layout/layout.html.twig' %}
{% block body %}
    {{ form_start(form) }}
    <div class="panel panel-default">
        <div class="panel-heading">{{ 'heading.new_speaker'|trans|desc("New Speaker") }}</div>
        <div class="panel-body">
            {{ form_widget(form) }}
        </div>
        <div class="panel-footer">
            <button type="submit" class="btn btn-primary">{{ 'button.save'|trans|desc("Save") }}</button>
        </div>
    </div>
    {{ form_end(form) }}
{% endblock %}